{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope":"off",
        "react/jsx-filename-extension":[1,{"extension": ["./js", ".jsx"]}],
        "jsx-quotes": [2, "prefer-single"]
    }
}
